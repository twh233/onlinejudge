#include <cstdio>
#include <cstring>
#include <algorithm>
using namespace std;

int main(){
    int l,r;
    scanf("%d%d",&l,&r);
    int ans = (l+r)/2;
    printf("%d\n",l-1);
    fflush(stdout);
    int flag;
    while(scanf("%d",&flag)!=EOF){
        if(flag == 0) break;
        printf("%d\n",l-1);
        fflush(stdout);
    }
    return 0;
}
